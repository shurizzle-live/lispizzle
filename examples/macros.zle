(defn test ()
  "this is a test"
  (let (t "calling test")
    (println t)))

(println (fn-name test))
(println (fn-doc  test))

(test)

(def x 2)

(println (primitive-eval '(+ x 1)))

(println (macroexpand '(primitive-eval '(+ x 1))))

(defmacro ++ (name)
  (let (curr (gensym))
    `(let (,curr ,name)
       (inc ,name)
       ,curr)))

(println (++ x))
(println x)

